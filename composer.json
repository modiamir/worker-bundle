{
    "name": "modiamir/worker-bundle",
    "type": "symfony-bundle",
    "description": "Create easy to user consumer and producer based on Rabbitmq Bundle",
    "keywords": ["bundle", "rabbitmq", "worker"],
    "homepage": "https://github.com/modiamir/worker-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/modiamir/worker-bundle/issues"
    },
    "require": {
        "php": ">=7.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "symfony/framework-bundle": "~2.1|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Modiamir\\WorkerBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
